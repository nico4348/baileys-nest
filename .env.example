# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=your_db_password
DATABASE_NAME=baileys_nest

# Redis Configuration
REDIS_URL=redis://default:your_redis_password@your-redis-host:port
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=your_redis_password

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET_NAME=your_bucket_name
S3_ENDPOINT=

# Environment
NODE_ENV=development
PORT=3000

# Session Configuration
SESSION_RECONNECTION_TIMEOUT=30000
SESSION_DELETE_TIMEOUT=10000
SESSION_RESTART_TIMEOUT=5000
SESSION_QR_TIMEOUT=120000
SESSION_MAX_RETRIES=3
SESSION_RETRY_DELAY=1000
SESSION_LOG_LEVEL=info

# Rate Limiting Configuration
DEFAULT_RATE_LIMIT=30
RATE_LIMIT_WINDOW=60
SESSION_QUEUE_CONCURRENCY=1
SESSION_QUEUE_CLEANUP_HOURS=24